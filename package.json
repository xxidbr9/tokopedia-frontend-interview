{
    "name": "weboo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "test": "turbo run test",
        "test:coverage": "turbo run test:coverage"
    },
    "devDependencies": {
        "eslint-config-weboo": "*",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {},
    "packageManager": "yarn@1.22.19"
}