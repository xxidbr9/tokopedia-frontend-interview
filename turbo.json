{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build",
                "$NODE_ENV"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "test": {
            "cache": false
        },
        "test:coverage": {
            "cache": false
        }
    }
}